package program7;

class StrongNumbers{
	
	public int factorial(int n)
	{
		int res=1;
		while(n!=0)
		{
			res*=n;
			n--;
		}
		return res;
	}
	public void isStrongNo(int num)
	{
		int digit;
		int sum=0;
		while(num!=0) {
			digit = num%10;
			num=num/10;
			
			sum+=factorial(num);
		}
	}
}
public class Program7 {
	
	public static void main(String[] args) {

	}
}
