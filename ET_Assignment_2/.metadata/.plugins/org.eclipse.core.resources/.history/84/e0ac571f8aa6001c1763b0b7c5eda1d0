
class Employee{
	private static String company = "ACI";
	private String empID;
	private String name;
	
	public Employee() {
		empID = null;
	}

	public String getCompany() {
		return company;
	}

	public void setCompany(String company) {
		Employee.company = company;
	}

	public String getEmpID() {
		return empID;
	}

	public void setEmpID(String empID) {
		this.empID = empID;
	}
	
}

